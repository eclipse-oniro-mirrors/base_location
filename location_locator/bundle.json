{
    "name": "@ohos/location_locator",
    "version": "3.1.0",
    "description": "Location Management Capability in Location Services.",
    "homePage": "https://gitee.com/openharmony",
    "license": "Apache License 2.0",
    "repository": "",
    "domain": "os",
    "language": "",
