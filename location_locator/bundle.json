            "SystemCapability.Location.Location"
        ],
        "features": [],
        "adapted_system_type": [
            "standard"
        ],
        "rom": "",
        "ram": "",
        "deps": {
            "components": [
                "ipc",
                "ces_standard",
                "hiviewdfx_hilog_native",
                "appexecfwk_standard",
